{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "automate-test"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/vendor/pace/pace.min.css",
        "assets/vendor/bootstrap/css/bootstrap.min.css",
        "assets/vendor/open-iconic/css/open-iconic-bootstrap.min.css",
        "assets/vendor/datatables/extensions/buttons/buttons.bootstrap4.min.css",
        "assets/vendor/font-awesome/css/fontawesome-all.min.css",
        "assets/vendor/flatpickr/flatpickr.min.css",
        "assets/stylesheets/main.min.css",
        "assets/stylesheets/custom.css",
        "styles.css"
      ],
      "scripts": [
        "assets/vendor/particles.js/particles.min.js",
        "assets/vendor/pace/pace.min.js",
        "assets/vendor/jquery/jquery.min.js",
        "assets/vendor/bootstrap/js/popper.min.js",
        "assets/vendor/bootstrap/js/bootstrap.min.js",
        "assets/vendor/stacked-menu/stacked-menu.min.js",
        "assets/vendor/perfect-scrollbar/perfect-scrollbar.min.js",
        "assets/vendor/flatpickr/flatpickr.min.js",
        "assets/vendor/jquery.easy-pie-chart/jquery.easypiechart.min.js",
        "assets/vendor/chart.js/Chart.min.js",
        "assets/vendor/datatables/jquery.dataTables.min.js",
        "assets/vendor/datatables/extensions/buttons/dataTables.buttons.min.js",
        "assets/vendor/datatables/extensions/buttons/buttons.bootstrap4.min.js",
        "assets/vendor/datatables/extensions/buttons/buttons.html5.min.js",
        "assets/vendor/datatables/extensions/buttons/buttons.print.min.js",
        "assets/javascript/pages/dataTables.bootstrap.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
